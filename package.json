{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/main.ts",
    "debug": {
      "watch": [
        "src"
      ],
      "ext": "ts",
      "ignore": [
        "src/**/*.spec.ts"
      ],
      "exec": "ts-node --inspect=5858 -r tsconfig-paths/register src/main.hmr.ts"
    },
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.3.0",
    "@nestjs/core": "^5.3.0",
    "@nestjs/jwt": "^0.3.0",
    "@nestjs/passport": "^5.0.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/testing": "^5.7.3",
    "@nestjs/typeorm": "^5.2.0",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.34",
    "@types/uuid": "^3.4.4",
    "bcrypt": "^3.0.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "config": "^3.0.1",
    "country-state-city": "^0.1.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.4.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.12",
    "typescript": "^3.2.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^7.0.41",
    "jest": "^20.0.4",
    "supertest": "^3.0.0",
    "ts-jest": "^20.0.14",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2"
  }
}
