{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/main.ts",
    "build": "tsc",
    "debug": "node $NODE_DEBUG_OPTION --inspect-brk -r tsconfig-paths/register -r ts-node/register src/main.ts",
    "clean": "node ./node_modules/rimraf/bin.js ./dist/",
    "tsc": "node ./node_modules/typescript/bin/tsc",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "typeorm:run": "ts-node ./node_modules/.bin/typeorm migration:run",
    "typeorm:revert": "ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.1",
    "@nestjs/core": "^6.0.1",
    "@nestjs/jwt": "^6.0.0",
    "@nestjs/passport": "^6.0.0",
    "@nestjs/platform-express": "^6.0.1",
    "@nestjs/swagger": "^3.0.1",
    "@nestjs/testing": "^6.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/config": "0.0.34",
    "@types/uuid": "^3.4.4",
    "bcrypt-nodejs": "0.0.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "config": "^3.0.1",
    "country-state-city": "^0.1.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0",
    "tsconfig-paths": "^3.8.0",
    "typeorm": "^0.2.12",
    "typescript": "^3.3.3333",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "jest": "^20.0.4",
    "supertest": "^3.0.0",
    "ts-jest": "^20.0.14",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.11.1"
  }
}
